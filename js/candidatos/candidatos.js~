$('.salirCandidatos').click(function () {
    $('#_alertSalirUsuarios .text-modal-body').html('Est√° seguro que desea salir del modulo de Candidatos?');
    $('#_alertSalirUsuarios').modal('show');
});

$("#addCandidatos").click(function() {
    window.location.href = 'index.php?r=candidatos/create';
});

$("#viewElecciones").click(function() {
    window.location.href = 'index.php?r=candidatos/index';
});
